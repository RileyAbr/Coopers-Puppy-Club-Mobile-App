{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "rules": {
        "react/prop-types": 0,
        "no-nested-ternary": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": false,
                "printWidth": 100,
                "endOfLine": "auto"
            }
        ]
    },
    "plugins": ["prettier"]
}
